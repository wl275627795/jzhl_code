@model MH.Rescue.BIZ.teaching_liv

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>直播课程详情</title>
</head>
<body>
    <div>
        <h4>直播课程详情</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                标题
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.title)
            </dd>
    
            <dt>
                描述
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.description)
            </dd>

            <dt>
                主讲人姓名
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_name)
            </dd>

            <dt>
                主讲人职称
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_title)
            </dd>

            <dt>
                主讲人科室
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_dept)
            </dd>

            <dt>
                主讲人单位
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_org)
            </dd>

            <dt>
                频道
            </dt>

            <dd>
                @Html.DisplayFor(model => model.channel_id)
            </dd>
    
            <dt>
                提交时间
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.created_dt)
            </dd>
    
            <dt>
                浏览次数
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.view_count)
            </dd>
    
            <dt>
                点赞次数
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.like_count)
            </dd>
    
            <dt>
                申请人数
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.apply_count)
            </dd>
    
            <dt>
                课程开始时间
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.start_dt)
            </dd>
    
            <dt>
                课程结束时间
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.end_dt)
            </dd>
    
            <dt>
                最大申请人数
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.apply_max)
            </dd>
    


        </dl>
    </div>
    <p>
        @Html.ActionLink("删除", "LiveDelete", new { id = Model.id }) |
        @Html.ActionLink("返回直播课程列表", "LiveList")
    </p>
</body>
</html>
