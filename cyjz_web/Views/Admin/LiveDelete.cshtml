@model MH.Rescue.BIZ.teaching_liv

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>直播课程已删除</title>
</head>
<body>
    <div>
        <h4>直播课程已删除</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                标题
            </dt>

            <dd>
                @Html.DisplayFor(model => model.title)
            </dd>

            <dt>
                描述
            </dt>

            <dd>
                @Html.DisplayFor(model => model.description)
            </dd>
    
            <dt>
                提交时间
            </dt>

            <dd>
                @Html.DisplayFor(model => model.created_dt)
            </dd>
    
           
    
            <dt>
                开始时间
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.start_dt)
            </dd>
    
            <dt>
                结束时间
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.end_dt)
            </dd>
    
            
            <dt>
                主讲人姓名
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_name)
            </dd>

            <dt>
                主讲人职称
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_title)
            </dd>

            <dt>
                主讲人科室
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_dept)
            </dd>

            <dt>
                主讲人单位
            </dt>

            <dd>
                @Html.DisplayFor(model => model.teacher_org)
            </dd>
    
  
        </dl>
    
        
    </div>
    <p>
        @Html.ActionLink("返回直播回放列表", "LiveList")
    </p>
</body>
</html>
